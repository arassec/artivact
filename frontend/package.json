{
  "name": "artivact",
  "version": "1.0.0-SNAPSHOT",
  "description": "A tool to create and maintain virtual artifacts.",
  "productName": "Artivact",
  "author": "Andreas Sensen <andreas.sensen@arassec.com>",
  "private": true,
  "scripts": {
    "lint": "eslint --fix --ext .js,.vue ./",
    "format": "prettier --write \"**/*.{js,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0",
    "dev": "quasar dev",
    "build": "quasar build",
    "dev:electron": "quasar dev -m electron",
    "build:electron": "quasar build -m electron",
    "build:all": "quasar build -m spa && quasar build -m electron --icon=./src-electron/icons/icon.png"
  },
  "dependencies": {
    "@google/model-viewer": "4.1.0",
    "@quasar/extras": "1.17.0",
    "axios": "1.13.5",
    "driver.js": "1.4.0",
    "markdown-it": "14.1.1",
    "pinia": "3.0.3",
    "quasar": "2.18.2",
    "three": "0.172.0",
    "vue": "3.5.18",
    "vue-i18n": "11.1.11",
    "vue-router": "4.5.1",
    "vuedraggable": "4.1.0"
  },
  "devDependencies": {
    "@electron/packager": "18.4.2",
    "@intlify/vue-i18n-loader": "5.0.0",
    "@quasar/app-vite": "2.3.0",
    "@types/markdown-it": "14.1.2",
    "@types/node": "24.3.0",
    "@typescript-eslint/eslint-plugin": "8.39.1",
    "@typescript-eslint/parser": "8.39.1",
    "autoprefixer": "10.4.21",
    "baseline-browser-mapping": "2.9.19",
    "electron": "^37.10.2",
    "electron-packager": "17.1.2",
    "eslint": "9.33.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-vue": "10.4.0",
    "prettier": "3.6.2",
    "typescript": "5.9.2"
  }
}
