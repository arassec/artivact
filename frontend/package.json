{
  "name": "artivact",
  "version": "1.0.0-SNAPSHOT",
  "description": "A tool to create and maintain virtual artifacts.",
  "productName": "Artivact",
  "author": "Andreas Sensen <andreas.sensen@arassec.com>",
  "private": true,
  "scripts": {
    "lint": "eslint --fix --ext .js,.vue ./",
    "format": "prettier --write \"**/*.{js,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0",
    "dev": "quasar dev",
    "build": "quasar build",
    "dev:electron": "quasar dev -m electron",
    "build:electron": "quasar build -m electron",
    "build:all": "quasar build -m spa && quasar build -m electron --icon=./src-electron/icons/icon.png"
  },
  "dependencies": {
    "@google/model-viewer": "4.1.0",
    "three": "0.172.0",
    "@quasar/extras": "1.16.17",
    "axios": "1.8.3",
    "markdown-it": "14.1.0",
    "pinia": "3.0.1",
    "quasar": "2.18.1",
    "vue": "3.5.13",
    "vue-i18n": "11.1.2",
    "vue-router": "4.5.0",
    "vuedraggable": "4.1.0"
  },
  "devDependencies": {
    "@electron/packager": "18.3.6",
    "@intlify/vue-i18n-loader": "5.0.0",
    "@quasar/app-vite": "2.1.4",
    "@types/node": "22.13.10",
    "@typescript-eslint/eslint-plugin": "8.26.1",
    "@typescript-eslint/parser": "8.26.1",
    "autoprefixer": "10.4.21",
    "electron": "35.0.1",
    "electron-packager": "17.1.2",
    "eslint": "9.22.0",
    "eslint-config-prettier": "10.1.1",
    "eslint-plugin-vue": "10.0.0",
    "prettier": "3.5.3",
    "typescript": "5.8.2"
  }
}
